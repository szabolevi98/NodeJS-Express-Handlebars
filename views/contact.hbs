<h4>Küldj nekünk üzenetet!</h4>
<form {{!--action="/action_page.php"--}} id="contactForm" onsubmit="return false">
    <label for="name">Név:</label>
    <input type="text" id="name" name="name" placeholder="A neved.">

    <label for="email">Email:</label>
    <input type="email" id="email" name="email" placeholder="Az e-mail címed.">

    <label for="phone">Telefon:</label>
    <input type="phone" id="phone" name="phone" placeholder="A telefonszámod.">

    <label for="subject">Tárgy:</label>
    <input type="text" id="subject" name="subject" placeholder="Az üzenet tárgya.">

    <label for="comment">Üzenet:</label>
    <textarea rows="4" cols="50" id="comment" name="comment" form="contactForm" placeholder="Írd be az üzenetet."></textarea>

    <input type="submit" value="Elküld" onclick="elkuld()">
</form>
<script src="{{htmlSet.scriptFolder}}/form.js"></script>
